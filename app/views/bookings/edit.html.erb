<div class="container">
  <h3>Edit booking</h3>
  <div class="row justify-content-center align-items-center mt-4" style="margin-bottom: 250px;">
    <div class="">
   <h3>Create a gym</h3>

    <%= simple_form_for [ @booking.gym, @booking ] do |f| %>
      <%= f.input :booking_start, as: :datetime, html5: true %>
      <%= f.input :booking_end, as: :datetime, html5: true %>
      <%= f.submit "Update", class: "btn-search btn-search-index" %>
    <% end %> 
    </div>
  </div>
</div>
    
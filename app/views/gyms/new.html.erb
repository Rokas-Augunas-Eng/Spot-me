<div class="container">
  <div class="row justify-content-center align-items-center mt-4" style="margin-bottom: 250px;">
    <div class="col-4">
   <h3>Create a gym</h3>
    <%= simple_form_for(@gym) do |f| %>
      <%= f.input :name %>
      <%= f.input :location %>
      <%= f.input :type_of_gym %>
      <%= f.input :equipment %>
      <%= f.submit %>
    <% end %> 
    </div>
  </div>
</div>
